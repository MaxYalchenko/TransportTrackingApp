<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.NewTrackingAppJava" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>
    <style name="Theme.NewTrackingAppJava" parent="Base.Theme.NewTrackingAppJava" />


    <style name="Theme.NewTrackingAppJava.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="WhiteButton" parent="Widget.AppCompat.Button.Colored">
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">22sp</item>
        <item name="android:padding">12dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
    </style>


    <style name="Theme.NewTrackingAppJava.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.NewTrackingAppJava.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="MyRequestDialog" parent="Theme.AppCompat.Light.Dialog.Alert">

        <item name="colorPrimary">@android:color/black</item>
        <item name="android:textColorPrimary">@android:color/black</item>

    </style>
</resources>